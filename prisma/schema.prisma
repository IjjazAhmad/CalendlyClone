
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model profile {
  id    String  @id @default(auto()) @map("_id") @db.ObjectId
  username  String?
  password String  @unique
}

model user {
  id    String  @id @default(auto()) @map("_id") @db.ObjectId
  email String  @unique
  password String 
}